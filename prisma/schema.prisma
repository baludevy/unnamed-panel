generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
}

model Server {
  id          String   @id
  containerId String   @unique
  name        String
  port        Int      @unique
  version     String
  type        String
  directory   String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
